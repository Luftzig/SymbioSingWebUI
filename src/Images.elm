tubes : FlowIO.Configuration -> List (Svg msg)
tubes configuration =
    case configuration of
        FlowIO.StandardConfiguration ->
            [ tubeInlet, tubeOutlet ]

        FlowIO.InflationSeries ->
            [ tubeInlet, tubeSeries ]

        FlowIO.InflationParallel ->
            [ tubeInlet, tubeParallelInf ]

        FlowIO.VacuumSeries ->
            [ tubeOutlet, tubeSeries ]

        FlowIO.VacuumParallel ->
            [ tubeOutlet, tubeParallelVac ]

        FlowIO.RegulatedPressure ->
            [ tubeInlet, tubeSeries ]

        FlowIO.RegulatedVacuum ->
            [ tubeOutlet, tubeSeries ]



deviceBox : FlowIO.ControlServiceStatus -> FlowIO.Configuration -> Svg msg
deviceBox { pump1, pump2, inlet, outlet, port1, port2, port3, port4, port5 } config =
    svg [ viewBox "0 0 1512 1332" ]
        ([ if pump1 then
            pump1On

           else
            pump1Off
         , if pump2 then
            pump2On

           else
            pump2Off
         , mainBody
         , if port1 then
            valve1On

           else
            valve1Off
         , if port2 then
            valve2On

           else
            valve2Off
         , if port3 then
            valve3On

           else
            valve3Off
         , if port4 then
            valve4On

           else
            valve4Off
         , if port5 then
            valve5On

           else
            valve5Off
         , if inlet then
            inletOn

           else
            inletOff
         , if outlet then
            outletOn

           else
            outletOff
         , Svg.path [ class "svg_air", fillRule "evenodd", fillOpacity "0.7", d "M204.3 287.28h1015.485v66.24H204.3z" ] []
         , g [ class "svg_greenOverlay", transform "translate(180.394 243.958)", fill "#409b68", fillRule "evenodd", stroke "#000", strokeWidth "2.16", strokeLinecap "square", strokeLinejoin "bevel", fillOpacity "0.2" ] [ rect [ width "1158.411", height "531.847", rx "25.614", ry "25.017" ] [], rect [ transform "matrix(1 0 0 .662 0 558.244)", width "1158.411", height "531.847", rx "25.614", ry "25.017" ] [] ]
         ]
            ++ tubes config
        )



